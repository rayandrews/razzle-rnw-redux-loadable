{
  "name": "razzle-examples-with-react-native-web",
  "version": "2.0.4",
  "license": "MIT",
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "clean:build": "rimraf build",
    "clean:node": "rimraf node_modules",
    "clean:all": "concurrently --kill-others \"yarn clean:build\" \"yarn clean:node\"",
    "pretty": "prettier-eslint --write \"src/**/*.js\"",
    "lint": "yarn pretty",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "express": "^4.15.2",
    "prop-types": "^15.6.1",
    "qs": "^6.5.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-loadable": "^5.4.0",
    "react-native-web": "^0.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^1.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-react-native-web": "^0.4.0",
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.0",
    "onchange": "^4.0.0",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1",
    "razzle": "^2.0.4",
    "rimraf": "^2.6.2"
  },
  "lint-staged": {
    "src/**/*.js": [
      "yarn lint",
      "git add"
    ]
  }
}
